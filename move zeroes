    void moveZeroes(vector<int>& nums) {
       int n=nums.size();
      
        int left=0,right=0;
        
        //right!=n-1 is not acceptable
        //because we will miss last element
        while(right<n){
            
            if(nums[right]==0) right++;
            else{
                swap(nums[left],nums[right]);
                left++;
                right++;
            }
        }
    }
